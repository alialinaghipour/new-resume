@model List<GetExperiencesViewModel>

<div class="section">
    <h2 class="title title--h2"><a class="fas fa-laptop-house"></a> تجربه</h2>
    <div class="timeline">
        @foreach (var item in Model)
        {
            <article class="timeline__item">
                <h5 class="title title--h3 timeline__title">@item.Title</h5>
                <span class="timeline__period">@item.StartDate - @item.EndDate</span>
                <p class="timeline__description">@item.Description</p>
            </article>
        }
    </div>
</div>
